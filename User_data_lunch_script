#!/bin/bash -ex
exec > >(tee /var/log/user-data.log|logger -t user-data -s 2>/dev/console) 2>&1
sudo yum upgrade
#install java
sudo yum install jdk-20-headless
#install git
sudo yum install git -y
cd /home/ec2-user
#clonwoking repo
git clone https://github.com/RainBowCreation/wokernode
cd worknode/node
#startup server
./run.sh
